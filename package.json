{
  "name": "shadcn-vue-echarts-workspace",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "docs"
  ],
  "scripts": {
    "build": "npm --workspaces --if-present run build",
    "test": "npm --workspaces --if-present run test",
    "typecheck": "npm --workspaces --if-present run typecheck",
    "lint": "npm --workspaces --if-present run lint",
    "format": "prettier --write \"**/*.{ts,tsx,vue,json,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build && npm run test && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vue-tsc": "^1.8.0"
  }
}
